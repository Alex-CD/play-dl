{
  "name": "play-dl",
  "version": "2.0.0",
  "description": "YouTube, SoundCloud, Spotify, Deezer searching and streaming for discord.js bots",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/play-dl/play-dl"
  },
  "author": "killer069 <65385476+killer069@users.noreply.github.com>",
  "license": "GPL-3.0",
  "scripts": {
    "lint": "prettier src --write && eslint src --ext .ts",
    "prepare": "husky install",
    "build": "tsup",
    "prepublishOnly": "tsup"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "node-youtube-dl",
    "youtube-dl",
    "yt-dl",
    "ytdl",
    "youtube",
    "spotify-dl",
    "spotify",
    "soundcloud",
    "deezer"
  ],
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.7.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "tsup": "^5.11.13",
    "typedoc": "^0.22.12",
    "typedoc-plugin-extras": "^2.2.3",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "^4.5.5",
    "yarn": "^1.22.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "play-audio": "^0.5.2"
  },
  "bugs": {
    "url": "https://github.com/play-dl/play-dl/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/play-dl/play-dl#readme"
